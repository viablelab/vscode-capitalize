{
  "name": "capitalize",
  "displayName": "Capitalize",
  "description": "Capitalize titles according to The Chicago Manual of Style",
  "version": "0.0.1",
  "license": "MIT",
  "publisher": "viablelab",
  "repository": "https://github.com/viablelab/vscode-capitalize",
  "engines": {
    "vscode": "^1.29.0"
  },
  "categories": [
    "Formatters"
  ],
  "activationEvents": [
    "onCommand:extension.capitalizeTitle"
  ],
  "main": "./out/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.capitalizeTitle",
        "title": "Capitalize Title"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "capitalize",
      "properties": {
        "capitalize.special": {
          "type": [
            "array"
          ],
          "default": [],
          "description": "A list of words that should be capitalized as specified."
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "yarn run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.42",
    "@types/node": "^8.10.25",
    "tslint": "^5.8.0",
    "typescript": "^2.6.1",
    "vscode": "^1.1.21"
  },
  "dependencies": {
    "title": "^3.4.0"
  }
}
